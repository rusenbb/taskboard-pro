{
	"name": "taskboard-pro",
	"version": "0.1.0",
	"description": "Kanban-style task board with Tasks plugin integration",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"test": "vitest run",
		"lint": "eslint src/",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"kanban",
		"tasks"
	],
	"author": "Rusen",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^20.10.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"builtin-modules": "^3.3.0",
		"esbuild": "^0.19.0",
		"eslint": "^8.50.0",
		"obsidian": "latest",
		"tslib": "^2.6.0",
		"typescript": "^5.3.0",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"rrule": "^2.8.1"
	}
}
